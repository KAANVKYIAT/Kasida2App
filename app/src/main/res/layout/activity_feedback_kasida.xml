<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Feedback_kasida">

    <ImageView
        android:id="@+id/button_feedback_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/we_want_feedback"/>

    <TextView
        android:layout_marginLeft="50dp"
        android:layout_below="@+id/button_feedback_1"
        android:id="@+id/text_product_feedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_linkProduct_feedback"
        android:textAlignment="center"
        android:layout_gravity="center_horizontal"
        android:textStyle="bold"
        android:fontFamily="sans-serif-black"
        android:textSize="40dp"
        android:textColor="@color/black"
        />


    <ImageView
        android:layout_width="600dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text_product_feedback"
        android:layout_marginTop="100dp"
        android:src="@drawable/kasida_team_image" />



  </RelativeLayout>